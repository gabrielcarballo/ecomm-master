{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend module",
  "main": "server.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig.json --outDir .build",
    "prestart": "npm run build",
    "start": "node .build/server.js",
    "predev": "npm run db:reset",
    "dev": "tsnd --watch \"./src/**\" --transpile-only ./src/server.ts",
    "sequelize": "sequelize",
    "sequelize:migrate": "npm run sequelize -- db:migrate",
    "sequelize:seed": "npm run sequelize -- db:seed:all",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "author": "gabrielcarballo",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "on-exit": "^1.0.1",
    "sequelize": "^6.6.5",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.11.7",
    "@types/pg": "^8.6.0",
    "@types/sequelize": "^4.28.11",
    "jest": "^27.2.5",
    "nodemon": "^2.0.22",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
